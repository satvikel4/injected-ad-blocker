{
    "manifest_version": 2,
    "name": "YouTube Adblock",
    "version": "1.0",
    "description": "Skips injected ads in YouTube videos",
    "permissions": ["activeTab", "https://www.youtube.com/*"],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }